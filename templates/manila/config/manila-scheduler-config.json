{
  "command": "/usr/bin/manila-scheduler --config-dir /etc/manila/manila.conf.d",
  "config_files": [
    {
      "source": "/var/lib/config-data/merged/manila.conf.d",
      "dest": "/etc/manila/manila.conf.d",
      "owner": "root:manila",
      "perm": "0750"
    },
    {
      "source": "/var/lib/config-data/merged/logging.conf",
